<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobmon">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Mobmon"
        android:usesCleartextTraffic="true">
        <activity android:name=".ui.about.AboutActivity"></activity>
        <activity android:name=".ui.dashboard.DashboardActivity"></activity>
        <activity android:name=".ui.profiles.ProfilesActivity"></activity>
        <activity android:name=".ui.settings.SettingsActivity"></activity>
        <activity android:name=".ui.support.SupportActivity"></activity>
        <activity android:name=".ui.widgets.WidgetsActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>